generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model FuelEntry {
  id         Int      @id @default(autoincrement())
  date       DateTime @db.Date
  kmCompteur Float
  litres     Float
  prixLitre  Float
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("fuel_entries")
}
